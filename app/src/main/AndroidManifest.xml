<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication2">

    <uses-permission android:name=" android.permission.READ_EXTERNAL_STORAGE " />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icone2"
        android:label="@string/to_contigo"
        android:roundIcon="@drawable/ic_icone2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".acticity.EnviarRegistros"
            android:label="@string/title_activity_enviar_registros"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>

        <activity
            android:name=".SplashTocontigo"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".acticity.registro"
            android:label="@string/title_activity_registro"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".perfil"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Telaquemsomos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>